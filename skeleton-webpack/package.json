{
  "name": "aurelia-skeleton-navigation-webpack",
  "version": "1.0.0-beta.1.3.1",
  "description": "A starter kit for building a standard navigation-style app with Aurelia and webpack.",
  "main": "dist/electron.js",
  "productName": "Aurelia Electron",
  "scripts": {
    "postinstall": "node ./config/select-language && npm run typings:install",
    "test": "npm run node-babel -- ./node_modules/karma/bin/karma start config/karma.conf.js",
    "webdriver:update": "cross-env ./node_modules/.bin/webdriver-manager update",
    "webdriver:start": "cross-env ./node_modules/.bin/webdriver-manager start",
    "link:typescript": "npm run link:clean && shx ln -s typescript-src src && shx ln -s typescript-test test",
    "link:javascript": "npm run link:clean && shx ln -s javascript-src src && shx ln -s javascript-test test",
    "link:clean": "shx rm -f ./src ./test",
    "pree2e": "npm run webdriver:update -- --standalone",
    "e2e": "cross-env ./node_modules/.bin/protractor ./config/protractor.conf.js",
    "e2e:live": "npm run e2e -- --elementExplorer",
    "clean": "npm cache clean && cross-env ./node_modules/.bin/rimraf node_modules typings coverage dist",
    "clean:dist": "cross-env ./node_modules/.bin/rimraf dist",
    "preclean:install": "npm run clean",
    "clean:install": "npm set progress=false && npm install",
    "preclean:start": "npm run clean",
    "clean:start": "npm start",
    "watch": "npm run watch:dev",
    "watch:dev": "npm run build:dev -- --watch",
    "watch:dev:hmr": "npm run watch:dev -- --hot",
    "watch:test": "npm run test -- --auto-watch --no-single-run",
    "watch:prod": "npm run build:prod -- --watch",
    "build": "npm run build:dev",
    "prebuild:dev": "npm run clean:dist",
    "build:dev": "npm run webpack -- --config config/webpack.development.js --progress --profile",
    "prebuild:prod": "npm run clean:dist",
    "build:prod": "npm run webpack -- --config config/webpack.production.js --progress --profile",
    "start": "npm run server:dev",
    "server": "npm run server:dev",
    "server:dev": "npm run webpack-dev-server -- --config config/webpack.development.js --inline --progress --profile --watch",
    "server:dev:hmr": "npm run server:dev -- --hot",
    "server:prod": "cross-env ./node_modules/.bin/http-server dist --cors",
    "electron:setup": "npm install source-map-support --save && npm install concurrently electron-debug electron-packager electron-prebuilt express minimist webpack-dev-middleware webpack-hot-middleware installed-local-npm-modules flat-npm-dependencies lodash --save-dev && npm run typings:install -- dt~github-electron dt~node --save-dev -G",
    "electron:start:hot": "cross-env HOT=1 NODE_ENV=development electron -r babel-register ./config/electron.entry.development",
    "electron:start:hot-server": "npm run node-babel -- config/electron.hot-server.js",
    "electron:start": "concurrently --kill-others \"npm run electron:start:hot\" \"npm run electron:start:hot-server\"",
    "preelectron:build": "npm run clean:dist",
    "electron:build": "npm run electron:build:main && npm run electron:build:renderer",
    "electron:build:main": "cross-env NODE_ENV=production npm run node-babel -- ./node_modules/webpack/bin/webpack --config config/webpack.electron.js --progress --profile",
    "electron:build:renderer": "cross-env NODE_ENV=production npm run node-babel -- ./node_modules/webpack/bin/webpack --config config/webpack.electron-renderer.production.js --progress --profile",
    "electron:package": "cross-env NODE_ENV=production npm run node-babel -- config/electron.package.js",
    "electron:package:all": "npm run package -- --all",
    "node-babel": "node -r babel-register",
    "webpack": "npm run node-babel -- ./node_modules/webpack/bin/webpack",
    "webpack-dev-server": "npm run node-babel -- ./node_modules/webpack-dev-server/bin/webpack-dev-server",
    "typings:install": "cross-env ./node_modules/.bin/typings install"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/aurelia/skeleton-navigation.git"
  },
  "keywords": [
    "aurelia",
    "skeleton",
    "navigation",
    "webpack"
  ],
  "license": "CC0-1.0",
  "author": "Rob Eisenberg <rob@bluespire.com> (http://robeisenberg.com/)",
  "contributors": [
    {
      "name": "Arjen de Blok"
    },
    {
      "name": "Bazyli Brz√≥ska <bazyli.brzoska@gmail.com> (https://invent.life)"
    }
  ],
  "bugs": {
    "url": "https://github.com/aurelia/skeleton-navigation/issues"
  },
  "homepage": "https://github.com/aurelia/skeleton-navigation#readme",
  "aurelia": {
    "build": {
      "resources": [
        {
          "path": "users",
          "bundle": "users",
          "lazy": true
        }
      ],
      "moduleRootOverride": {
        "aurelia-templating-resources": "dist/es2015",
        "aurelia-templating-router": "dist/es2015"
      }
    }
  },
  "dependencies": {
    "aurelia-bootstrapper-webpack": "^1.0.0-rc.1.0.0",
    "aurelia-event-aggregator": "^1.0.0-rc.1.0.0",
    "aurelia-fetch-client": "^1.0.0-rc.1.0.0",
    "aurelia-framework": "^1.0.0-rc.1.0.0",
    "aurelia-history-browser": "^1.0.0-rc.1.0.0",
    "aurelia-loader-webpack": "^1.0.0-rc.1.0.0",
    "aurelia-logging-console": "^1.0.0-rc.1.0.0",
    "aurelia-pal-browser": "^1.0.0-rc.1.0.0",
    "aurelia-polyfills": "^1.0.0-rc.1.0.0",
    "aurelia-route-recognizer": "^1.0.0-rc.1.0.0",
    "aurelia-router": "^1.0.0-rc.1.0.0",
    "aurelia-templating-binding": "^1.0.0-rc.1.0.0",
    "aurelia-templating-resources": "^1.0.0-rc.1.0.0",
    "aurelia-templating-router": "^1.0.0-rc.1.0.0",
    "bluebird": "^3.4.1",
    "bootstrap": "^4.0.0-alpha.2",
    "font-awesome": "^4.6.3",
    "isomorphic-fetch": "^2.2.1",
    "jquery": "^3.0.0",
    "regenerator-runtime": "^0.9.5",
    "ts-helpers": "^1.1.1"
  },
  "devDependencies": {
    "aurelia-tools": "^0.2.3",
    "aurelia-webpack-plugin": "^1.0.0-beta.2.0.3",
    "awesome-typescript-loader": "^2.0.0-rc.10",
    "babel-core": "^6.10.4",
    "babel-eslint": "^6.0.5",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-runtime": "^6.9.0",
    "babel-polyfill": "^6.9.1",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-es2015-loose": "^7.0.0",
    "babel-preset-es2015-loose-native-modules": "^1.0.0",
    "babel-preset-stage-1": "^6.5.0",
    "babel-register": "^6.9.0",
    "bundle-loader": "^0.5.4",
    "compression-webpack-plugin": "^0.3.1",
    "copy-webpack-plugin": "^3.0.1",
    "cross-env": "^1.0.8",
    "css-loader": "^0.23.1",
    "del": "^2.2.1",
    "eslint": "^2.13.1",
    "eslint-loader": "^1.3.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "html-loader": "^0.4.3",
    "html-webpack-plugin": "^2.21.0",
    "http-server": "^0.9.0",
    "inquirer": "^1.0.3",
    "jasmine-core": "^2.4.1",
    "json-loader": "^0.5.4",
    "jsonfile": "^2.3.1",
    "karma": "^0.13.22",
    "karma-chrome-launcher": "^1.0.1",
    "karma-jasmine": "^1.0.2",
    "karma-mocha-reporter": "^2.0.4",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^1.7.0",
    "npm-run-all": "^2.2.0",
    "offline-plugin": "^3.4.0",
    "protractor": "^3.3.0",
    "raw-loader": "^0.5.1",
    "recursive-readdir-sync": "^1.0.6",
    "rimraf": "^2.5.2",
    "shx": "^0.1.2",
    "source-map-loader": "^0.1.5",
    "style-loader": "^0.13.1",
    "ts-babel-node-extendable": "^1.0.0",
    "tslint": "^3.11.0",
    "tslint-loader": "^2.1.5",
    "typescript": "^1.9.0-dev.20160622-1.0",
    "typings": "^1.3.0",
    "url-loader": "^0.5.7",
    "wallaby-webpack": "0.0.22",
    "webpack": "^2.1.0-beta.13",
    "webpack-dev-server": ">=2.1.0-beta.0 || ^2.1.0",
    "webpack-md5-hash": "0.0.5",
    "webpack-merge": "^0.14.0"
  }
}
